.wrapper {
  display: flex;
  justify-content: center;
  position: relative;
  width: 164px;
  height: 128px;
  border-radius: 4px;
  border: 1px solid #40a9ff; 
  background: white;
  & > .label {
    flex: 1;
    height: 28px;
    line-height: 28px;
    text-align:center;
    color: white;
    background: #40a9ff;
  };
  & > .expandButton {
    width: 21px;
    height: 21px;
    position: absolute;
    bottom: 0;
    // bottom: -25px;
    // z-index: 99;
  }
}