.groupWrapper {
  width: 95%;
  min-height: 100px;
  background: white;
  border: 1px solid #d9d9d9;
  margin: 10px 1% 10px 4%;
  border-radius: 4px;  
  position: relative;
  padding: 0;
  &:not(.head)::before {
    content: '';
    position: absolute;
    left: -1.5%;
    width: 1.5%;
    height: 130%;
    top: calc(-75%);
    border: 0 solid #CCC;
    border-width: 0 0 2px 2px;
  }
  &.head {
    width: 100%;
    height: 100%;
    margin: 0;
  }
  & > .operatorWrapper {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    width: 100%;
    height: 36px;
    & .button{
      line-height:26px;
      height:26px;
      padding:0;
      text-align:center;
    }
    & .leftButton{
      width:50px
    }
    & .rightButton{
      width:100px;
    }
    & .deleteButton{
      width:30px;
    }
  }
}