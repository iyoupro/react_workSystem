.wrapperWidth{
  width: 1200px;
  height: 704px;
}
.wrapperWidthFit{
  width: fit-content;
  height: fit-content;
}
.wrapper{
  margin: auto;
  border-radius:4px;
  border: 1px solid gray;
  transition:height 0.5s;
  & :global(.ant-tabs-tab) {
    padding:8px 4px !important;
    text-overflow: ellipsis !important;
  }
  & .leftHead{
    position: absolute;
    margin: 8px -106px;
    font-weight: bold;
    font-size: 16px;
  }
  & :global(.ant-tabs-tab){
    color:rgba(0,0,0,0.65);
    margin-bottom: 0px !important;
    text-align: center !important;
  }
  & :global(.ant-tabs-nav-wrap){
      width:104px;
      padding-top:32px;
      text-align: center;

    }
  & :global(.ant-tabs) {
    height: calc(~'100% - 24px');
  };
  & :global(.ant-tabs-content) {
    height: 100%;
  }
  & :global(.ant-tabs-tabpane) {
    height: 100%;
  }
  & .footer{
    height: 24px;
    line-height: 24px;
    border-bottom-left-radius: 4px;
    border-bottom-right-radius: 4px;
    background: #e8e8e8;
    text-align: center;
    // border: 1px solid red;
    cursor: pointer;
    &:hover{
      font-size: 15px;
      // font-weight:bold;
    }

    // & .footIcon:hover{
    //   transform: scale(1.3);
    //   font-weight:bold;
    // }
  };
};