import React from 'react';
import ReactQuill, { Quill, Mixin, Toolbar } from 'react-quill';
import { withState, pipe, withHandlers } from '../rehook';
import 'react-quill/dist/quill.snow.css'; // ES6
import styles from './QuillPage.less';
let fonts = ['songTi', 'heiTi','kaiTi','fangSong'];  
let Font = Quill.import('formats/font');  
Font.whitelist = fonts;
Quill.register(Font, true);
let Size = Quill.import('attributors/style/size');
Size.whitelist = ['12px', '24px', '36px', '48px'];
Quill.register(Size, true);

const enhance = pipe(
  withState('value', 'setValue', '<div style=\"margin:30px auto;width: 730px;\">\r\n    <!--start-->\r\n    <div style=\"width: 600px;height: 800px;background: white;z-index: 2;padding: 2rem  4rem;\">\r\n      <img src=\"data:image/png;base64,\" style=\"margin-top:-30px;\"/>\r\n      <span style=\"  margin-left: 23%;font-size: 2rem;\"> 聘 书</span>\r\n      <p style=\"font-size: 1.2rem;text-indent:2rem;\">兹聘任钟豪先生/女士为维修工程部教员，聘期年，自至止。</p>\r\n      <br/>\r\n      <p style=\"text-indent:2rem;font-size: 1.2rem;\">特发此证</p>\r\n      <p style=\"padding-left: 55%;font-size: 1.2rem;\">华夏航空维修工程部培训分部</p>\r\n      <p  style=\"padding-left: 65%;font-size: 1.2rem;\"></p>\r\n      <br>\r\n      <span style=\" margin-left: 40%;font-size: 1.6rem;\">续聘记录</span>\r\n      <table  border=\"1\" cellspacing=\"0\" style=\"margin-left: 10%;margin-top: 1%; margin-bottom: 10%;\">\r\n        <tr>\r\n          <td style=\"text-align: center;width: 220px;height: 30px;\">续聘日期</td>\r\n          <td style=\"text-align: center;width: 220px;height: 30px;\">有效期至</td>\r\n        </tr>\r\n        <tr>\r\n          <td style=\"text-align: center;width: 220px;height: 30px;\">2019-11-21</td>\r\n          <td style=\"text-align: center;width: 220px;height: 30px;\">2020-11-21</td>\r\n        </tr>\r\n        <tr>\r\n          <td style=\"text-align: center;width: 220px;height: 30px;\">2020-11-22</td>\r\n          <td style=\"text-align: center;width: 220px;height: 30px;\">2021-11-22</td>\r\n        </tr>\r\n        <tr>\r\n          <td style=\"text-align: center;width: 220px;height: 30px;\">2021-11-23</td>\r\n          <td style=\"text-align: center;width: 220px;height: 30px;\">2022-11-23</td>\r\n        </tr>\r\n      </table>\r\n      <p style=\"font-weight:bold;text-indent:2rem;font-size: 1.2rem;\">备注：</p>\r\n      <span style=\"display: block;width:500px;margin-left:10%;\">续聘周期到期前2个月系统需要提醒（邮件、微信、短信，频率为每周一次） 培训管理员续签聘书，培训管理员如同意续聘，则系统自动生成下一周期（三年） 的续聘记录，并录入系统。续聘日期是对上一聘用日期截止时间进行连续续聘， 而不是培训管理员处理流程日期，如上一聘用日期为：2015年1月1日至2018年1月1日，\r\n        培训管理人员处理流程的时间是2017年10月1日，那么续聘日期应为：2018年1月1日至 2021年1月1日\r\n      </span>\r\n    </div>\r\n    <!--end-->\r\n  </div>'),
  withHandlers({
    onChange: ({ setValue }) => (value) => {
      setValue(value);
      console.log(value);
    }
  })
);

// var toolbarOptions = [
//   ['bold', 'italic', 'underline', 'strike'],        // toggled buttonsqqqq
//   ['blockquote', 'code-block'],

//   [{ 'header': 1 }, { 'header': 2 }],               // custom button values
//   [{ 'list': 'ordered' }, { 'list': 'bullet' }],
//   [{ 'script': 'sub' }, { 'script': 'super' }],      // superscript/subscript
//   [{ 'indent': '-1' }, { 'indent': '+1' }],          // outdent/indent
//   [{ 'direction': 'rtl' }],                         // text direction

//   [{ 'size': ['small', false, 'large', 'huge', 'aaa'] }],  // custom dropdown
//   [{ 'header': [1, 2, 3, 4, 5, 6, false] }],

//   [{ 'color': [] }, { 'background': [] }],          // dropdown with defaults from theme
//   [{ 'font': [] }],
//   [{ 'align': [] }],

//   ['clean']                                         // remove formatting button
