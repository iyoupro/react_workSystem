.wrapper {
  width: 100%;
  min-height: 100%;
  background: #f5f5f5;
  padding-top: 100px;
  & > .collapse {
    margin: auto;
    width: 80%;
    background: transparent;
    & .header {
      display: flex;
      flex-direction: row;
      flex-wrap: nowrap;
      align-items: center;
      user-select: none;
      & > hr {
        margin: 0 12px;
        width: 100%;
        flex: 1;
        border-color: #d9d9d9;
        opacity: 0.3;
      };
      & > .headerIcon + .headerIcon {
        margin-left: 12px;
      };
    };
    & .panel {
      border-bottom: 0;
      & .item + .item {
        border-top: 1px solid rgba(0, 0, 0, 0.075);
      };
      & .item {
        display: flex;
        flex-direction: row;
        justify-content: space-between;
        position: relative;
        min-height: 56px;
        margin-left: 24px;
        padding: 12px 92px 12px 60px;
        background: #fff;
        & > .checkbox {
          position: absolute;
          left: 24px;
        };
        & > .textArea {
          width: 30%;
          & > .title {
            font-size: 14px;
            font-weight: 600;
            margin: 0;
          };
        
          & > .description {
            margin: 8px 0 0 0;
            font-size: 13px;
            color: rgba(0, 0, 0, 0.5);
          };
        
        };
        & > .labelArea {
          display: flex;
          flex-direction: row-reverse;
          flex-wrap: wrap;
          align-content: flex-start;
          width: 30%;
          min-height: 34px;
          & > p {
            height: fit-content;
            margin-right: 12px;
            padding: 0px 6px;
            border-radius: 5px;
            background: #95de64;
            font-size: 12px;
            font-weight: 400;
            color: #fafafa;
          }
        };
        & > .timeIcon {
          position: absolute;
          right: 64px;
          color: #ffa39e;
        };
        & > .timeText {
          position: absolute;
          transform: translateY(-2.5px);
          right: 20px;
          color: #ffa39e;
        }
      };
    };
  };
};