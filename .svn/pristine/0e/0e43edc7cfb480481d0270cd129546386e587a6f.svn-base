.title {
  font-size: 14px;
  color: #5a5a5a;
  margin-top: 18px;
}



.item {
  display: flex !important;
  align-items: center !important;
  .icon {
    display: none !important;
  }
  &:hover {
    .icon {
      display: flex !important;
      justify-content: space-around;
      align-items: center;
      color: #bbbbbb;
    }
  }
}

.itemCheck {
  display: flex;
  align-items: center;
  .icon {
    display: none;
  }
}

.itemSelect {
  display: flex;
  align-items: center;
  .icon {
    display: none;
  }
}

.radio {
  font-size: 14px;
  color: #365169;
  

}

.radioGroup {
  width: 100%;
  padding-top: 12px;
  padding-left:16px;
  margin-bottom: 12px;
}

.distance {
  padding-bottom:10px;
  &:last-child{
    padding-bottom:0;
  }
}

.inputNumber{
  margin-top: 12px;
}

