/** @format */

.content {
  width: 40%;
  height: 60px;
  margin: auto;
  margin-top: 24px;
  border: 1px solid black;
  border-radius: 4px;
  display: flex;
  justify-content: center;
  align-items: center;
  transition: all 0.4s ease;
  font-size: 18px;
  &:hover {
    border: 1px solid #1890ff;
    // background: #1890ff;
    color: #1890ff;
    cursor: pointer;
  }
}
