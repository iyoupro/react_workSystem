.wrapperWidth{
  width: 1200px;
  height: 700px;
}
.wrapperWidthFit{
  width: fit-content;
  height: fit-content;
}
.wrapper{
  margin: auto;
  border: 1px solid gray;
  & .leftHead{
    position: absolute;
    margin: 8px -106px;
    font-weight: bold;
    font-size: 16px;
  }
  & :global(.ant-tabs-tab){
    color:rgba(0,0,0,0.65);
    margin-bottom: 0px !important;
    text-align: center !important;
  }
  & :global(.ant-tabs-nav-wrap){
      width:104px;
      padding-top:32px;
      text-align: center;

    }
  & :global(.ant-tabs) {
    height: calc(~'100% - 20px');
  };
  & :global(.ant-tabs-content) {
    height: 100%;
  }
  & :global(.ant-tabs-tabpane) {
    height: 100%;
  }
  & .footer{
    height: 20px;
    background: #e8e8e8;
    text-align: center;
    cursor: pointer;
    & .footIcon:hover{
      transform: scale(1.3);
      font-weight:bold;
    }
  };
};