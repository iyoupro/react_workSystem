
.titleText{
  flex:1;
	font-size: 16px;
	font-weight: normal;
  color: #333;
  display: inline-block;
  text-align: left;  
}

.score{
  flex:1;
	font-size: 24px;
	font-weight: 600;
  color: #ffc540;
  display: inline-block;
  text-align: right;
  span{
    font-size: 14px;
    font-weight: normal;
    line-height: 14px;
    margin-left:2px;
  }
}

.title{               
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  padding: 0 24px;
  border-radius: 2px;
  border-bottom: 1px solid #d9d9d9;
}

.titleContent{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height:48px;
  line-height: 48px;
 
}

