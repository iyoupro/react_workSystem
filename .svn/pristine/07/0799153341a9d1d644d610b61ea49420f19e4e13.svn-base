.QuillWrapper {
  :global(.quill) {
    margin: auto; 
    width: 950px; 
    height: 300px;
  }
  :global(.ql-container.ql-snow) {
    border-top: none;
  }
  :global(.ql-toolbar.ql-snow) {
    border-bottom: none;
    width: 950px;
    margin: auto;
    background: rgba(153,153,153,0.6);
  }

  /* 不能改变三个样式的顺序 */
  :global(.ql-editor .ql-font-songTi) {
    font-family: Georgia, "Nimbus Roman No9 L", "Songti SC", "Noto Serif CJK SC", "Source Han Serif SC", "Source Han Serif CN", STSong, "AR PL New Sung", "AR PL SungtiL GB", NSimSun, SimSun, "TW\-Sung", "WenQuanYi Bitmap Song", "AR PL UMing CN", "AR PL UMing HK", "AR PL UMing TW", "AR PL UMing TW MBE", PMingLiU, MingLiU, serif;
  };
  :global(.ql-snow .ql-picker.ql-font .ql-picker-label[data-value="songTi"]::before) {
    content: '宋体';
  }
  :global(.ql-snow .ql-picker.ql-font .ql-picker-item[data-value="songTi"]::before) {
    content: '宋体';
    font-family: Georgia, "Nimbus Roman No9 L", "Songti SC", "Noto Serif CJK SC", "Source Han Serif SC", "Source Han Serif CN", STSong, "AR PL New Sung", "AR PL SungtiL GB", NSimSun, SimSun, "TW\-Sung", "WenQuanYi Bitmap Song", "AR PL UMing CN", "AR PL UMing HK", "AR PL UMing TW", "AR PL UMing TW MBE", PMingLiU, MingLiU, serif;
  }
  
  :global(.ql-editor .ql-font-heiTi) {
    font-family: -apple-system, "Noto Sans", "Helvetica Neue", Helvetica, "Nimbus Sans L", Arial, "Liberation Sans", "PingFang SC", "Hiragino Sans GB", "Noto Sans CJK SC", "Source Han Sans SC", "Source Han Sans CN", "Microsoft YaHei", "Wenquanyi Micro Hei", "WenQuanYi Zen Hei", "ST Heiti", SimHei, "WenQuanYi Zen Hei Sharp", sans-serif;
  };
  :global(.ql-snow .ql-picker.ql-font .ql-picker-label[data-value="heiTi"]::before) {
    content: '黑体';
  }
  :global(.ql-snow .ql-picker.ql-font .ql-picker-item[data-value="heiTi"]::before) {
    content: '黑体';
    font-family: -apple-system, "Noto Sans", "Helvetica Neue", Helvetica, "Nimbus Sans L", Arial, "Liberation Sans", "PingFang SC", "Hiragino Sans GB", "Noto Sans CJK SC", "Source Han Sans SC", "Source Han Sans CN", "Microsoft YaHei", "Wenquanyi Micro Hei", "WenQuanYi Zen Hei", "ST Heiti", SimHei, "WenQuanYi Zen Hei Sharp", sans-serif;
  };

  :global(.ql-editor .ql-font-fangSong) {
    font-family: Baskerville, "Times New Roman", "Liberation Serif", STFangsong, FangSong, FangSong_GB2312, "CWTEX\-F", serif;
  };
  :global(.ql-snow .ql-picker.ql-font .ql-picker-label[data-value="fangSong"]::before) {
    content: '仿宋';
    font-family: Baskerville, "Times New Roman", "Liberation Serif", STFangsong, FangSong, FangSong_GB2312, "CWTEX\-F", serif;
  }
  :global(.ql-snow .ql-picker.ql-font .ql-picker-item[data-value="fangSong"]::before) {
    content: '仿宋';
    font-family: Baskerville, "Times New Roman", "Liberation Serif", STFangsong, FangSong, FangSong_GB2312, "CWTEX\-F", serif;
  };
  
  :global(.ql-editor .ql-font-kaiTi) {
    font-family: Baskerville, Georgia, "Liberation Serif", "Kaiti SC", STKaiti, "AR PL UKai CN", "AR PL UKai HK", "AR PL UKai TW", "AR PL UKai TW MBE", "AR PL KaitiM GB", KaiTi, KaiTi_GB2312, DFKai-SB, "TW\-Kai", serif;
  };
  :global(.ql-snow .ql-picker.ql-font .ql-picker-label[data-value="kaiTi"]::before) {
    content: '楷体';
    font-family: Baskerville, Georgia, "Liberation Serif", "Kaiti SC", STKaiti, "AR PL UKai CN", "AR PL UKai HK", "AR PL UKai TW", "AR PL UKai TW MBE", "AR PL KaitiM GB", KaiTi, KaiTi_GB2312, DFKai-SB, "TW\-Kai", serif;
  }
  :global(.ql-snow .ql-picker.ql-font .ql-picker-item[data-value="kaiTi"]::before) {
    content: '楷体';
    font-family: Baskerville, Georgia, "Liberation Serif", "Kaiti SC", STKaiti, "AR PL UKai CN", "AR PL UKai HK", "AR PL UKai TW", "AR PL UKai TW MBE", "AR PL KaitiM GB", KaiTi, KaiTi_GB2312, DFKai-SB, "TW\-Kai", serif;
  };
  


  
  /* 不能改变三个样式的顺序 */
  :global(.ql-editor .ql-size-12px) {
    font-size: 12px;
  }
  :global(.ql-snow .ql-picker.ql-size .ql-picker-label[data-value="12px"]::before) {
    content: '12px';
  }
  :global(.ql-snow .ql-picker.ql-size .ql-picker-item[data-value="12px"]::before) {
    content: '12px';
    font-size: 12px;
  }
  
  :global(.ql-editor .ql-size-24px) {
    font-size: 24px;
  }
  :global(.ql-snow .ql-picker.ql-size .ql-picker-label[data-value="24px"]::before) {
    content: '24px';
  }
  :global(.ql-snow .ql-picker.ql-size .ql-picker-item[data-value="24px"]::before) {
    content: '24px';
    font-size: 24px;
  }
  
  :global(.ql-editor .ql-size-36px) {
    font-size: 36px;
  }
  :global(.ql-snow .ql-picker.ql-size .ql-picker-label[data-value="36px"]::before) {
    content: '36px';
  }
  :global(.ql-snow .ql-picker.ql-size .ql-picker-item[data-value="36px"]::before) {
    content: '36px';
    font-size: 36px;
  }
  
  :global(.ql-editor .ql-size-48px) {
    font-size: 48px;
  }
  :global(.ql-snow .ql-picker.ql-size .ql-picker-label[data-value="48px"]::before) {
    content: '48px';
  }
  :global(.ql-snow .ql-picker.ql-size .ql-picker-item[data-value="48px"]::before) {
    content: '48px';
    font-size: 48px;
  }  
}