.groupWrapper {
  width: calc(~"100% - 48px - 12px");
  min-height: 100px;
  background: white;
  border: 1px solid #d9d9d9;
  margin: 10px 4px 10px 48px;
  border-radius: 4px;  
  position: relative;
  &:not(.head)::before {
    content: '';
    position: absolute;
    left: -24px;
    width: 24px;
    height: calc(~"50% + 12px"); // margin-top + border-width = 12px;
    top: calc(~"-12px");
    border: 0 solid #CCC;
    border-width: 0 0 2px 2px;
  }
  &.first::before {
    height: calc(~"50% + 24px");
    top: calc(~"-24px");
  }
  &:not(.head):not(.last)::after {
    content: '';
    position: absolute;
    left: -24px;
    width: 24px;
    height: 50%;
    top: calc(50%);
    border: 0 solid #CCC;
    border-width: 0 0 0 2px;
  }
  &.head {
    width: 100%;
    height: 100%;
    margin: 0;
  }
  & > .operatorWrapper {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    width: 100%;
    height: 36px;
    padding: 0 12px 0 12px;
    & .button{
      line-height:26px;
      height:26px;
      padding:0;
      text-align:center;
    }
    & .leftButton{
      width:50px
    }
    & .rightButton{
      width:100px;
    }
    & .deleteButton{
      width:30px;
    }
  }
}