.wrapper {
  width: 100%;
  height: 100%;
  background: #fffbe6;
  display: flex;
  align-items: center;
  justify-content: center;
  border: 1px solid black;
  border-radius: 4px;
  font-weight: bold;
  &:hover {
    background: #91d5ff;
  };
  &.dragging {
    opacity: 0.3;
  };
  &.selected {
    // border: 1px solid red;
    background: #096dd9;
  };
  &.preView {
    background: #d9f7be;
    border: 1px dashed black;
  };
  &.draggedOverSelf {
    background: #ff7875;
  };
  &.placeholder {
    background: #5cdbd3;
  }
  transition: all 0.3s ease;
}