.topBox{
  // border: 1px solid gray;
  font-size: 13px;
  width: 100%;
  height: 100%;
  background: #f5f5f5;
  display: flex;
  align-items: center;
  // justify-content: center;
  padding-left: 10px;
  .aaa{
    display: none;
    margin-left: 5px;
  }

  &.dragging {
    opacity: 0.3;
  };
  &.selected {
    // border: 1px solid red;
    background: #bfbfbf;
  };
  &:hover {
    background: #bae7ff;
    .aaa{
      display: block;
    }
  };
  &.preView {
    background: #d9f7be;
    border: 1px dashed black;
  };
  &.draggedOverSelf {
    background: #ff7875;
  };
  &.placeholder {
    background: #5cdbd3;
  }
  transition: all 0.3s ease;
}