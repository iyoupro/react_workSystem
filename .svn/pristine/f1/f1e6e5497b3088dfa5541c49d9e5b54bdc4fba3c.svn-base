.groupWrapper {
  width: 95%;
  min-height: 100px;
  background: white;
  border: 1px solid #d9d9d9;
  margin: 10px 1% 10px 4%;
  border-radius: 4px;  
  position: relative;
  padding: 0;
  &.head {
    width: 100%;
    height: 100%;
    margin: 0;
  }
  & > .verticalLine {
    position: absolute;
    background: black;
    left: 2%;
    width: 2px;
    height: calc(~"100% - 8px");
    margin-block-start: 6px;
    z-index: 1;
  }
  & > .horizontalLine {
    position: absolute;
    background: black;
    top: calc(~"50% - 0.5px");
    left: -2.1%;
    width: 1.7%;
    height: 1px;
    margin-block-start: 0;
    z-index: 0;
  }
  & > .operatorWrapper {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    width: 100%;
    height: 36px;
    & .button{
      line-height:26px;
      height:26px;
      padding:0;
      text-align:center;
    }
    & .leftButton{
      width:50px
    }
    & .rightButton{
      width:100px;
    }
    & .deleteButton{
      width:30px;
    }
  }
}