.wrapper {
  width: 100%;
  height: 800px;
  background: white;
  position: relative;
  & > .panel {
    z-index: 999;
    display: flex;
    flex-direction: row;
    align-items: center;
    position: absolute;
    width: fit-content;
    height: 36px;
    top: 12px;
    right: 24px;
    & > .zoomSlider {
      width: 100px;
    }
    & > .select {
      margin-left: 12px;
      width: 77px;
    }
  }
}