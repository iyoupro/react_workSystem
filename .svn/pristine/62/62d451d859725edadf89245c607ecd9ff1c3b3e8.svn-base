.groupWrapper {
  width: 96%;
  min-height: 100px;
  background: white;
  border: 1px solid #d9d9d9;
  margin: 10px 1% 10px 3%;
  border-radius: 4px;  
  &.head{
    width: 100%;
    height: 100;
    margin: 0;
  }
  & > .operatorWrapper {
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    & .button{
      line-height:26px;
      height:26px;
      padding:0;
      text-align:center;
    }
    & .leftButton{
      width:50px
    }
    & .rightButton{
      width:100px;
    }
    & .deleteButton{
      width:30px;
    }
  }
}