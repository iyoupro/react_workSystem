.wrapper {
  position: relative;
  padding: 0;
  width: 60%;
  height: fit-content;
  box-shadow: 0px 0px 5px 0.5px grey;
  background: white;
  list-style: none;
  padding-inline-start: 0;
  padding-inline-end: 0;
  margin-block-end: 0;
  margin-block-start: 0;
  overflow: visible;
  // &.dragging {
  //   background: #f5f5f5;
  // }
  &.notEdit {
    padding: 0 0 48px 0;
  }
  & .totalScore {
    z-index: 2;
    position: absolute;
    top: 4px;
    right: 44px;
    font-size: 48px;
    color: #ffc53d;
  }
  & .ItemWrapper {
    position: relative;
    // &.dragging {
    //   & li {
    //     opacity: 0.3;
    //   }
    // };
    & .dragIcon{
      display: none;
      padding-top: 12px;
      position: absolute;
      cursor: move;
      height: 24px;
      font-size: 16px;
      width: 100%;
      color: #bfbfbf;
      z-index: 10;
      &.couldDrag {
        display: inline;
      }
    }
  }
};
