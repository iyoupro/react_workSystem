.wrapper {
  padding: 0;
  width: 60%;
  // transform: translateY(-12px);
  margin-top: -12px !important;
  height: fit-content;
  box-shadow: 0px 0px 5px 0.5px grey;
  list-style: none;
  padding-inline-start: 0;
  padding-inline-end: 0;
  margin-block-end: 0;
  margin-block-start: 0;
  overflow: visible;
  & .ItemWrapper {
    position: relative;
    & .dragIcon{
      padding-top: 12px;
      position: absolute;
      cursor: move;
      height: 24px;
      font-size: 16px;
      width: 100%;
      color: #bfbfbf;
      z-index: 10;
    }
  }
};
