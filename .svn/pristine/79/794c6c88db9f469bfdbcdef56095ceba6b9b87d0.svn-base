.wrapper{
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  & .radioLine{
    padding: 4px 0;
    display: flex;
    position: relative;
    width: 100%;
    &:hover{
      & .closeIcon{
        display: inline;
      }
    }
    & .closeIcon{
      cursor: pointer;
      font-size: 16px;
      line-height: 32px;
      width: 32px;
      position: absolute;
      top: 0px;
      right: 20px;
      // display: none;
    }
  };
  &  .radioAdd{
    margin-right: 20px;    
    &:hover{
      // outline: dashed;
      cursor: pointer;
      color: gray;
      border-bottom:1px dashed #000;
    };
  }

} 