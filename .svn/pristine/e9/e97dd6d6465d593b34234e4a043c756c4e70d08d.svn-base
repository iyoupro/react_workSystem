@import '~antd/lib/style/themes/default.less';
.componentLib {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 20%;
  height: 75%;
  background: #fff;
  // box-shadow: 1px 1px 1px 1px grey;
  border-radius: 4px;
  border:1px solid #d9d9d9;
  margin: 0 24px;
}

.buttonWrapper {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
  width: 90%;
  height: 15%;
  // background: blue;
  border-bottom: 1px solid #e8e8e8;
  margin-bottom: 5%;
}

.fixComponent{
  width: 90%; 
  height: 80%; 
  background: #fff;
  justify-content: space-between;
}

.button {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  width: 30%;  
  height: 60%;
  border: 1px solid #d9d9d9;
  // box-shadow: 1px 1px 1px 1px grey;
  // background: #1890ff;
  color: #595959;
  // font-weight: bold;
  // border-radius: 4px;
  transition: all 0.5s ease;
  &:hover {
    cursor: pointer;
    background: #e6f7ff !important;
  }
  
}

.component{
  width: 60%;
  height: 100%;
  background: #fff;
  border: 1px solid #d9d9d9
}
.attribute{
  border:1px solid red;
  width:15%;
  height: 100%;
  margin-left: 24px;
}